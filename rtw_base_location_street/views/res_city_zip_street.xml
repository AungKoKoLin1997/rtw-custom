<odoo>
    <data>
        <record id="city_zip_form_street" model="ir.ui.view">
            <field name="name">res.city.zip.form.street</field>
            <field name="model">res.city.zip</field>
            <field name="inherit_id" ref="base_location.city_zip_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='city_id']" position="after">
                    <field name="street"/>
                </xpath>
            </field>
        </record>
        <record id="city_zip_tree_street" model="ir.ui.view">
            <field name="name">res.city.zip.tree.street</field>
            <field name="model">res.city.zip</field>
            <field name="inherit_id" ref="base_location.city_zip_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='city_id']" position="after">
                    <field name="street"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>