<odoo>
    <data>
        <record model="ir.ui.view" id="stock_move_line_list">
            <field name="name">move_line_list</field>
            <field name="model">stock.move</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="sale_order_no" string="受注注番"/>
                    <field name="production_no" string="発注注番"/>
                    <field name="product_tmpl_id" string="商品名"/>
                    <field name="config_value_ids" widget="many2many_tags" string="属性"/>
                    <field name="product_uom_qty" string="数量"/>
                    <field name="sai" sum="sai" string="才数"/>
                    <field name="warehouse_id" string="製作工場"/>
                    <field name="state" string="製作状況"/>
                    <field name="state_id" string="エリア"/>
                    <field name="destination" string="到着地"/>
                    <field name="shiratani_date" string="白谷"/>
                    <field name="depo_date" string="デポ入れ日"/>
                    <field name="date_planned" string="搬入設置日"/>
                    <field name="delivery_confirmation" string="納期確認"/>
                    <field name="remarks" string="備考"/>
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="stock_move_line_action_window">
            <field name="name">move_line_list</field>
            <field name="res_model">stock.move</field>
            <field name="view_id" ref="stock_move_line_list" />
            <field name="view_mode">tree,kanban,form</field>
        </record>

        <menuitem name="move_line"
                  id="move_line_action_window_menu"
                  action="stock_move_line_action_window"
                  parent="stock.menu_stock_warehouse_mgmt"/>
    </data>
</odoo>